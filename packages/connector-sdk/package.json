{
  "name": "@magda/connector-sdk",
  "description": "MAGDA Connector SDK",
  "version": "2.2.2",
  "scripts": {
    "prebuild": "rimraf dist tsconfig.tsbuildinfo",
    "compile": "webpack && webpack --env.target=web",
    "dts-rollup": "api-extractor run -l && api-extractor run -c api-extractor-web.json -l",
    "build": "yarn compile && yarn dts-rollup",
    "release": "npm publish || echo \"Skip releasing npm package @magda/connector-sdk.\""
  },
  "author": "",
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "browser": "dist/index-web.js",
  "devDependencies": {
    "@magda/typescript-common": "^2.2.2",
    "@microsoft/api-extractor": "7.15.2",
    "ts-loader": "^6.2.1",
    "typescript": "~4.2.4",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "@types/urijs": "^1.19.19",
    "@types/yargs": "^12.0.8"
  },
  "magda": {
    "language": "typescript",
    "categories": {
      "npmPackage": true,
      "useCommonLib": true
    }
  },
  "keywords": [
    "Magda",
    "Connector",
    "SDK"
  ],
  "files": [
    "dist"
  ]
}
