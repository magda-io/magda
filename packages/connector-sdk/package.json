{
  "name": "@magda/connector-sdk",
  "description": "MAGDA Connector SDK",
  "version": "3.0.2-alpha.1",
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "engines": {
    "node": ">=18.19.0"
  },
  "scripts": {
    "prebuild": "rimraf dist tsconfig.tsbuildinfo",
    "build": "yarn build-main && yarn build-types && yarn build-types-bundle",
    "build-main": "node esbuild.js",
    "build-types": "tsc --emitDeclarationOnly",
    "build-types-bundle": "api-extractor run --local",
    "release": "npm publish || echo \"Skip releasing npm package @magda/connector-sdk.\""
  },
  "author": "",
  "license": "Apache-2.0",
  "devDependencies": {
    "@magda/typescript-common": "^3.0.2-alpha.1",
    "@microsoft/api-extractor": "~7.39.0",
    "@types/yargs": "^12.0.8",
    "esbuild": "^0.19.10",
    "ts-loader": "^9.5.1",
    "typescript": "~5.3.3"
  },
  "dependencies": {
    "@types/urijs": "^1.19.19"
  },
  "magda": {
    "language": "typescript",
    "categories": {
      "npmPackage": true,
      "useCommonLib": true
    }
  },
  "keywords": [
    "Magda",
    "Connector",
    "SDK"
  ],
  "files": [
    "dist"
  ]
}
