{
  "name": "@magda/utils",
  "description": "MAGDA Common Utils",
  "version": "0.0.60-alpha.0",
  "scripts": {
    "prebuild": "rimraf dist tsconfig.tsbuildinfo",
    "compile": "webpack && webpack --env.target=web",
    "dts-rollup": "api-extractor run -l && api-extractor run -c api-extractor-web.json -l",
    "build": "yarn compile && yarn dts-rollup",
    "release": "npm publish || echo \"Skip releasing npm package @magda/utils.\""
  },
  "author": "",
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "browser": "dist/index-web.js",
  "devDependencies": {
    "@magda/typescript-common": "^0.0.60-alpha.0",
    "@microsoft/api-extractor": "^7.7.8",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.2",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10"
  },
  "dependencies": {
    "@types/request": "^2.48.1"
  },
  "magda": {
    "language": "typescript",
    "categories": {
      "npmPackage": true,
      "useCommonLib": true
    }
  },
  "files": [
    "dist"
  ]
}
