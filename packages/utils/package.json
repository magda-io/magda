{
  "name": "@magda/utils",
  "description": "MAGDA Common Utils",
  "version": "6.0.0-alpha.3",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "browser": "./dist/index-web.js"
    },
    "./package.json": "./package.json"
  },
  "engines": {
    "node": ">=18.19.0"
  },
  "scripts": {
    "prebuild": "rimraf dist tsconfig.tsbuildinfo",
    "build": "yarn build-main && yarn build-types && yarn build-types-bundle",
    "build-main": "node esbuild.js",
    "build-types": "tsc --emitDeclarationOnly",
    "build-types-bundle": "api-extractor run --local",
    "release": "npm publish || echo \"Skip releasing npm package @magda/utils.\""
  },
  "author": "",
  "license": "Apache-2.0",
  "module": "dist/index.js",
  "browser": "dist/index-web.js",
  "devDependencies": {
    "@magda/typescript-common": "^6.0.0-alpha.3",
    "@microsoft/api-extractor": "~7.39.0",
    "esbuild": "^0.19.10",
    "ts-loader": "^9.5.1",
    "typescript": "~5.3.3"
  },
  "dependencies": {
    "@types/request": "^2.48.1"
  },
  "magda": {
    "language": "typescript",
    "categories": {
      "npmPackage": true,
      "useCommonLib": true
    }
  },
  "files": [
    "dist"
  ]
}
