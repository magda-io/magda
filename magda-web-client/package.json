{
  "name": "@magda/web-client",
  "description": "The MAGDA in-browser web front end.",
  "version": "0.0.53-0",
  "license": "Apache-2.0",
  "scripts": {
    "storybook": "start-storybook -p 9001 -c .storybook -s ./public",
    "deploy-storybook": "build-storybook -c .storybook -o .out",
    "dev": "yarn run start",
    "build-staging": "env REACT_APP_IS_STAGING=1 yarn run compile",
    "compile": "yarn run build-css && react-app-rewired build",
    "prepare": "pancake",
    "start": "react-scripts start",
    "build": "pancake && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@gov.au/accordion": "^3.0.5",
    "@gov.au/animate": "^1.0.0",
    "@gov.au/body": "^2.0.7",
    "@gov.au/breadcrumbs": "^2.0.3",
    "@gov.au/buttons": "^3.0.1",
    "@gov.au/core": "^3.0.0",
    "@gov.au/cta-link": "^2.1.1",
    "@gov.au/footer": "^2.1.3",
    "@gov.au/grid-12": "^2.0.5",
    "@gov.au/header": "^4.1.1",
    "@gov.au/link-list": "^2.1.1",
    "@gov.au/page-alerts": "^2.0.5",
    "@gov.au/pancake": "^1.2.2",
    "@gov.au/pancake-json": "^1.0.5",
    "@gov.au/pancake-react": "^1.1.2",
    "@gov.au/pancake-sass": "^1.3.2",
    "@gov.au/select": "^2.0.5",
    "@gov.au/skip-link": "^2.0.6",
    "@gov.au/tags": "^3.1.1",
    "@gov.au/text-inputs": "^2.0.6",
    "@types/papaparse": "^4.5.7",
    "chrono-node": "^1.3.11",
    "core-js": "^2.5.7",
    "i18next": "^11.10.1",
    "is-url-external": "^1.0.3",
    "leaflet": "^0.7.7",
    "react-app-polyfill": "^0.2.1",
    "react-i18next": "^8.1.0",
    "urijs": "^1.19.1"
  },
  "devDependencies": {
    "@magda/scripts": "^0.0.53-0",
    "@storybook/addon-actions": "^3.3.3",
    "@storybook/addon-links": "^3.3.3",
    "@storybook/addons": "^3.3.3",
    "@storybook/react": "^3.2.17",
    "@types/react": "16.4.9",
    "babel-core": "^6.26.0",
    "browser-detect": "^0.2.22",
    "d3-collection": "^1.0.4",
    "deep-freeze": "0.0.1",
    "dompurify": "^1.0.2",
    "echarts": "^4.1.0",
    "echarts-for-react": "^2.0.14",
    "email-validator": "^2.0.3",
    "eslint": "^5.14.1",
    "expect": "^22.4.0",
    "fast-xml-parser": "^2.6.0",
    "fetch-mock": "^5.8.1",
    "flow-bin": "^0.39.0",
    "history": "^4.7.2",
    "html-truncate": "^1.2.2",
    "js-cookie": "^2.2.0",
    "leaflet-mapbox-vector-tile": "github:TerriaJS/Leaflet.MapboxVectorTile",
    "lodash": "^4.17.10",
    "markdown-it": "^8.4.0",
    "markdown-with-front-matter-loader": "^0.1.0",
    "memoize-one": "^3.1.1",
    "node-sass": "^4.9.0",
    "npm-run-all": "^4.0.1",
    "openlayers": "^4.1.1",
    "papaparse": "^4.3.3",
    "pretty-date": "^0.2.0",
    "prop-types": "^15.6.1",
    "query-string": "^4.3.4",
    "raf": "^3.4.0",
    "react": "^16.3.2",
    "react-app-rewire-eslint": "^0.2.3",
    "react-app-rewired": "^1.1.4",
    "react-autosuggest": "^9.3.2",
    "react-document-title": "^2.0.3",
    "react-dom": "^16.3.2",
    "react-ga": "^2.5.3",
    "react-json-tree": "^0.10.9",
    "react-modal": "^3.4.5",
    "react-particles-js": "^2.1.0",
    "react-pdf": "^2.0.0-alpha.7",
    "react-pdf-js": "^1.0.37",
    "react-redux": "^5.0.7",
    "react-responsive": "^4.1.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "^2.1.5",
    "react-table": "^6.5.1",
    "react-transition-group": "^2.3.1",
    "redux": "^4.0.0",
    "redux-devtools-extension": "^2.13.2",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.2.1",
    "redux-thunk": "^2.1.0",
    "rss-parser": "^2.9.0",
    "sass-versioning": "^0.3.0",
    "traverse": "^0.6.6",
    "uuid": "^3.0.1",
    "weighted-mean": "^1.0.2",
    "yaml-front-matter": "^4.0.0"
  },
  "browserslist": [
    "last 2 versions",
    "ie 11",
    "chrome 41"
  ],
  "pancake": {
    "auto-save": true,
    "plugins": true,
    "ignore": [],
    "json": {
      "enable": true,
      "location": "src/pancake/",
      "name": "pancake",
      "content": {
        "name": true,
        "version": true,
        "dependencies": true,
        "path": true,
        "settings": true
      }
    },
    "css": {
      "minified": true,
      "modules": false,
      "browsers": [
        "last 2 versions",
        "ie 8",
        "ie 9",
        "ie 10"
      ],
      "location": "src/pancake/css/",
      "name": false
    },
    "sass": {
      "modules": true,
      "location": "src/pancake/sass/",
      "name": "pancake.scss"
    },
    "js": {
      "minified": true,
      "modules": false,
      "location": "src/pancake/js/",
      "name": "pancake.min.js"
    },
    "react": {
      "location": "src/pancake/react/"
    }
  },
  "config": {
    "docker": {
      "include": "node_modules build package.json"
    },
    "magda": {
      "language": "javascript",
      "categories": {
        "ui": true
      }
    }
  }
}
